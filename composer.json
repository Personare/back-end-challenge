{
    "name": "rafaelpv/back-end-challenge",
    "description": "Desafio para os futuros back-end's do Personare",
    "keywords": ["rafaelpv", "challenge", "api"],
    "license": "AGPL-3.0",
    "type": "project",
    "require": {
        "php": ">=5.6",
        "laravel/lumen-framework": "5.2.*",
        "vlucas/phpdotenv": "~2.2",
        "mlntn/lumen-artisan-serve": "~1"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~4.0",
        "phpspec/phpspec": "~2.1",
        "squizlabs/php_codesniffer": "^2.3",
        "phploc/phploc": "*",
        "sebastian/phpcpd": "*",
        "phpmd/phpmd": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    }
